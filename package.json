{
  "name": "ts-playwright-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate project for TS Playwright",
  "scripts": {
    "test-ab-testing": "npx playwright test specs/abTesting.spec.ts",
    "test-add-remove-elements": "npx playwright test specs/addRemoveElements.spec.ts",
    "test-basic-auth": "npx playwright test specs/basicAuth.spec.ts",
    "test-broken-images": "npx playwright test specs/brokenImages.spec.ts",
    "test-challenging-dom": "npx playwright test specs/challengingDOM.spec.ts",
    "test-checkboxes": "npx playwright test specs/checkboxes.spec.ts",
    "test-context-menu": "npx playwright test specs/contextMenu.spec.ts",
    "test-regression-run": "npx playwright test specs",
    "alllure-generate": "allure generate ./artifacts/allure-results -o ./artifacts/allure-report --clean"
  },
  "keywords": [
    "AQA",
    "TypeScript",
    "Playwright"
  ],
  "author": "Maksym Kuznetsov",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.9.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "allure-playwright": "^2.9.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "husky": "^8.0.0",
    "typescript": "^5.3.2"
  }
}
